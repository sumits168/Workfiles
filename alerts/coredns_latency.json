{
  "displayName": "coredns latency",
  "userLabels": {},
  "conditions": [
    {
      "displayName": "New condition",
      "conditionPrometheusQueryLanguage": {
        "duration": "3600s",
        "evaluationInterval": "30s",
        "query": "# Latency: 95%\nhistogram_quantile(0.95,\nsum(rate(coredns_dns_request_duration_seconds_bucket[10m])) by (le, cluster)\n) < 0.1"
      }
    }
  ],
  "alertStrategy": {
    "notificationPrompts": [
      "OPENED"
    ]
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": [],
  "severity": "SEVERITY_UNSPECIFIED"
}