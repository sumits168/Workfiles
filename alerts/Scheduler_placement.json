{
  "displayName": "Scheduler: reliable placement (copy)",
  "userLabels": {},
  "conditions": [
    {
      "displayName": "New condition",
      "conditionPrometheusQueryLanguage": {
        "duration": "3600s",
        "evaluationInterval": "30s",
        "query": "# Scheduling Attempts (success rate)\n(sum(rate(scheduler_schedule_attempts_total{result=\"scheduled\"}[10m])) by (cluster)\n/\nsum(rate(scheduler_schedule_attempts_total[10m])) by (cluster) \n) * 100 > 95"
      }
    }
  ],
  "alertStrategy": {
    "notificationPrompts": [
      "OPENED"
    ]
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": [],
  "severity": "SEVERITY_UNSPECIFIED"
}