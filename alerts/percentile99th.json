{
  "displayName": "99 Percentile latency",
  "userLabels": {},
  "conditions": [
    {
      "displayName": "New condition",
      "conditionPrometheusQueryLanguage": {
        "duration": "3600s",
        "evaluationInterval": "30s",
        "query": "histogram_quantile(0.99,\nsum(rate(apiserver_request_duration_seconds_bucket[5m])) by (verb, le)) > 100"
      }
    }
  ],
  "alertStrategy": {
    "notificationPrompts": [
      "OPENED"
    ]
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": [],
  "severity": "WARNING"
}