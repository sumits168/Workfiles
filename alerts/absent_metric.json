{
  "displayName": "Absent metric alerting test (copy)",
  "userLabels": {},
  "conditions": [
    {
      "displayName": "Absence",
      "conditionAbsent": {
        "filter": "resource.type = \"prometheus_target\" AND metric.type = \"prometheus.googleapis.com/apiserver_request_total/counter\"",
        "aggregations": [
          {
            "alignmentPeriod": "300s",
            "crossSeriesReducer": "REDUCE_NONE",
            "perSeriesAligner": "ALIGN_RATE"
          }
        ],
        "duration": "300s",
        "trigger": {
          "count": 1
        }
      }
    }
  ],
  "alertStrategy": {
    "notificationPrompts": [
      "OPENED"
    ]
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": [],
  "severity": "SEVERITY_UNSPECIFIED"
}